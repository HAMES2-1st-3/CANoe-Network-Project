//chassis Gateway

on message CAN3::ACU_crashstate{
  message *msg;
  msg=this;
  msg.can=2;// CAN2(body)로 채널 변경 BCM
  
  output(msg);
}


on message CAN3.chassis::HECU_brake{
  message *msg;
  msg=this;
  msg.can=1;// CAN1(pt)로 채널 변경  ECM_M, ECM_S
  
  output(msg);
}

on message CAN3.chassis::CLU_odometer{
  message *msg;
  msg=this;
  msg.can=1;// CAN1(pt)로 채널 변경  ECM_M , ECM_S
  
  output(msg);
}


