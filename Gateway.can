{
  message * msg;
  msg=this; //현재 받은 메시지 그대로 msg에 넣어줌
  msg.can=3; // CAN3(chassis)로 채널 변경 CLU
  
  output(msg);  //그대로 전송해줌(게이트웨이역할)
}

